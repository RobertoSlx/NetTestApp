<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppTest03.Views.Login.LoginFacebookPage"
             xmlns:vm="clr-namespace:AppTest03.ViewModels">

    <ContentPage.BindingContext>
		<vm:LoginFacebookPage_VM/>
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
		<StackLayout>
			<Button x:Name="LoginButtFB" Text="Iniciar con Facebook"
					Clicked="LoginButtFB_Clicked"/>
			<WebView x:Name="WVFace" HeightRequest="10" WidthRequest="10" VerticalOptions="FillAndExpand"/>

		</StackLayout>
    </ContentPage.Content>
</ContentPage>